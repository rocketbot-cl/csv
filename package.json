      "form": {
        "css": "modal-lg",
        "inputs": [
          {
            "type": "file_select",
            "placeholder": {
              "es": "C:/User/Usuario/Folder/file.csv",
              "en": "C:/User/Usuario/Folder/file.csv"
            },
            "title": {
              "es": "Ruta del archivo: ",
